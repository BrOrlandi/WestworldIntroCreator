{
  "name": "WestworldIntroCreator",
  "version": "1.0.0",
  "description": "Westword Intro Creator by Kassel Labs",
  "repository": "git@github.com:BrOrlandi/WestworldIntroCreator.git",
  "author": "Kassel Labs <kassellabs@googlegroups.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rm -rf ./dist && parcel build ./src/index.html --public-url /WestworldIntroCreator/",
    "build-dev": "yarn build --public-url /",
    "start": "parcel ./src/index.html -p 8080",
    "test": "jest",
    "test:watch": "yarn test --watchAll",
    "test:coverage": "yarn test --coverage",
    "test:coverage:results": "google-chrome coverage/lcov-report/index.html",
    "lint": "eslint .",
    "deploy": "yarn build && ghpages -p dist"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "ghpages": "^0.0.10",
    "jest": "^22.4.2",
    "parcel-bundler": "^1.7.0",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  }
}
