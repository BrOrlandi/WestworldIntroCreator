{
  "name": "westworld-intro-creator",
  "version": "1.0.0",
  "description": "Westword Intro Creator by Kassel Labs",
  "repository": "git@github.com:BrOrlandi/WestworldIntroCreator.git",
  "author": "Kassel Labs <kassellabs@googlegroups.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rm -rf ./dist && parcel build ./src/index.html --public-url /WestworldIntroCreator/ --no-cache",
    "build-dev": "yarn build --public-url / --no-cache",
    "start": "parcel ./src/index.html -p 8080 --no-hmr",
    "test": "jest",
    "test:watch": "yarn test --watchAll",
    "test:coverage": "yarn test --coverage",
    "test:coverage:results": "google-chrome coverage/lcov-report/index.html",
    "lint": "eslint .",
    "deployp": "yarn build && cp CNAME dist/ && gh-pages -d dist -m ",
    "deploy": "yarn build && gh-pages -d dist -m "
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "gh-pages": "^1.2.0",
    "jest": "^23.1.0",
    "parcel-bundler": "^1.9.2",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-react": "^6.24.1",
    "fscreen": "^1.0.2",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "react": "^16.4.0",
    "react-context-connector": "^0.1.2",
    "react-dom": "^16.4.0",
    "react-fullscreen-crossbrowser": "^1.0.9",
    "react-router-dom": "^4.2.2",
    "react-youtube": "^7.5.0",
    "sweetalert2": "7.17"
  }
}
